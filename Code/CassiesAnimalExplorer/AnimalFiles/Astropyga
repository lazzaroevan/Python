[('Astropyga radiata', 'Astropyga radiata, the red urchin, fire urchin, false fire urchin or blue-spotted urchin, is a species of sea urchin in the family Diadematidae. It is a large species with long spines and is found in the tropical Indo-Pacific region. It was first described in 1778 by the German naturalist Nathaniel Gottfried Leske.', ['https://upload.wikimedia.org/wikipedia/commons/0/05/Astropyga_radiata_Q_Phia.jpg', 'https://upload.wikimedia.org/wikipedia/commons/1/12/Astropyga_radiata_group.JPG', 'https://upload.wikimedia.org/wikipedia/commons/3/39/Astropyga_radiata3.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/7/75/Astropyga_radiata_Lembeh.JPG', 'https://upload.wikimedia.org/wikipedia/commons/9/90/False_Fire_Urchin_%2820_cm%29.png', 'https://upload.wikimedia.org/wikipedia/commons/9/9f/Astropyga_radiata.jpg', 'https://upload.wikimedia.org/wikipedia/commons/a/a2/Astropyga_radiata%2C_black_and_red.JPG', 'https://upload.wikimedia.org/wikipedia/commons/b/b8/Astropyga_radiata2.jpg', 'https://upload.wikimedia.org/wikipedia/commons/d/d7/Astropyga_radiata_out_of_water.JPG', 'https://upload.wikimedia.org/wikipedia/en/4/4a/Commons-logo.svg']), ('Astropyga magnifica', 'Astropyga magnifica is a species of sea urchins of the family Diadematidae. Their armour is covered with spines. Astropyga magnifica was first scientifically described in 1934 by Austin Hobart Clark.', ['https://upload.wikimedia.org/wikipedia/commons/4/4b/Riccio_Melone_a_Capo_Caccia_adventurediving.it.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/c/cf/Astropyga_magnifica.jpg']), ('Astropyga nuptialis', 'Astropyga nuptialis is a species of sea urchins of the family Diadematidae. Their armour is covered with spines. Astropyga nuptialis was first scientifically described in 1958 by Tommasi.', ['https://upload.wikimedia.org/wikipedia/commons/4/4b/Riccio_Melone_a_Capo_Caccia_adventurediving.it.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png']), ('Astropyga pulvinata', 'Astropyga pulvinata is a species of sea urchins of the family Diadematidae. Their armour is covered with spines. Astropyga pulvinata was first scientifically described in 1816 by Jean-Baptiste de Lamarck.', ['https://upload.wikimedia.org/wikipedia/commons/4/4b/Riccio_Melone_a_Capo_Caccia_adventurediving.it.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/7/75/Astropyga_pulvinata.jpg']), ('Astropyga radiata', 'Astropyga radiata, the red urchin, fire urchin, false fire urchin or blue-spotted urchin, is a species of sea urchin in the family Diadematidae. It is a large species with long spines and is found in the tropical Indo-Pacific region. It was first described in 1778 by the German naturalist Nathaniel Gottfried Leske.', ['https://upload.wikimedia.org/wikipedia/commons/0/05/Astropyga_radiata_Q_Phia.jpg', 'https://upload.wikimedia.org/wikipedia/commons/1/12/Astropyga_radiata_group.JPG', 'https://upload.wikimedia.org/wikipedia/commons/3/39/Astropyga_radiata3.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/7/75/Astropyga_radiata_Lembeh.JPG', 'https://upload.wikimedia.org/wikipedia/commons/9/90/False_Fire_Urchin_%2820_cm%29.png', 'https://upload.wikimedia.org/wikipedia/commons/9/9f/Astropyga_radiata.jpg', 'https://upload.wikimedia.org/wikipedia/commons/a/a2/Astropyga_radiata%2C_black_and_red.JPG', 'https://upload.wikimedia.org/wikipedia/commons/b/b8/Astropyga_radiata2.jpg', 'https://upload.wikimedia.org/wikipedia/commons/d/d7/Astropyga_radiata_out_of_water.JPG', 'https://upload.wikimedia.org/wikipedia/en/4/4a/Commons-logo.svg'])]