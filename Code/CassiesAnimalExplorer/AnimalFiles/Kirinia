[('Kirinia', 'Kirinia is a genus of butterflies of the family Nymphalidae found in Europe and Asia.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/c/c6/Kirinia_roxelana_male_1.jpg']), ('Kirinia climene', 'Kirinia climene, the Iranian Argus, is a butterfly of the family Nymphalidae. It was described by Eugenius Johann Christoph Esper in 1783. It is distributed in Bulgaria, Romania, Yugoslavia, Macedonia, Turkey, Caucasus, Transcaucasia, Syria, Iraq, Iran and southern Russia.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/c/c4/Kirinia_climene_male.jpg', 'https://upload.wikimedia.org/wikipedia/commons/d/df/Wikispecies-logo.svg', 'https://upload.wikimedia.org/wikipedia/en/4/4a/Commons-logo.svg', 'https://upload.wikimedia.org/wikipedia/en/6/62/PD-icon.svg']), ('Kirinia epaminondas', 'Kirinia epaminondas is a Palaearctic butterfly of the family Nymphalidae. It was described by Otto Staudinger in 1887.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/archive/7/74/20120404173704%21Red_Pencil_Icon.png']), ('Kirinia epimenides', 'Kirinia epimenides  is a butterfly in the family Nymphalidae (Satyrinae). It is found in the East Palearctic in Amur, eastern China, Korea, Ussuri, Japan.', ['https://upload.wikimedia.org/wikipedia/commons/6/6c/406-Kirinia_fentoni.JPG', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png']), ('Kirinia roxelana', 'Kirinia roxelana, the lattice brown, is a butterfly of the family Nymphalidae. It is found in southeastern Europe and the Near East. The butterfly is on wing between May and July. The larvae feed on various grasses.', ['https://upload.wikimedia.org/wikipedia/commons/6/67/Kirinia_Roxelana.JPG', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/c/c6/Kirinia_roxelana_male_1.jpg', 'https://upload.wikimedia.org/wikipedia/commons/d/df/Wikispecies-logo.svg', 'https://upload.wikimedia.org/wikipedia/en/4/4a/Commons-logo.svg', 'https://upload.wikimedia.org/wikipedia/en/6/62/PD-icon.svg'])]