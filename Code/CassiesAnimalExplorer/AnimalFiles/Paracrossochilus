[('Paracrossochilus', 'Paracrossochilus is a genus of cyprinid fishes containing two species, both of which are endemic to Borneo.', ['https://upload.wikimedia.org/wikipedia/commons/0/07/LabeoCalbasuDay.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png']), ('Paracrossochilus acerus', 'Paracrossochilus acerus is a species of cyprinid in the genus Paracrossochilus. It inhabits Malaysia and has a maximum length of 9.5 centimetres (3.7 in). It is found in large schools, and is considered harmless to humans.', ['https://upload.wikimedia.org/wikipedia/commons/0/07/LabeoCalbasuDay.jpg', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png']), ('Paracrossochilus vittatus', 'Paracrossochilus vittatus is a species of cyprinid in the genus Paracrossochilus. It inhabits Indonesian Borneo and has a maximum length of 7.5 centimetres (3.0 in). Its habitat is fast flowing freshwater and it is considered harmless to humans.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/7/74/Rutilus_rutilus_Lithuania_resized.jpg'])]