[('Kiriakoffalia', 'Kiriakoffalia is a genus of tiger moths in the family Erebidae. The genus was erected by Hervé de Toulgoët in 1978. The moths in the genus are found in central Africa.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/b/b4/Spilosoma_sp._%285722639526%29.jpg', 'https://upload.wikimedia.org/wikipedia/commons/d/df/Wikispecies-logo.svg', 'https://upload.wikimedia.org/wikipedia/en/4/4a/Commons-logo.svg']), ('Kiriakoffalia costimacula', 'Kiriakoffalia costimacula is a moth of the family Erebidae. It was described by James John Joicey and George Talbot in 1924. It is found in the Democratic Republic of the Congo, Kenya, Rwanda and Uganda.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/b/b4/Spilosoma_sp._%285722639526%29.jpg']), ('Kiriakoffalia guineae', 'Kiriakoffalia guineae is a moth of the  family Erebidae. It was described by Strand in 1912. It is found in Equatorial Guinea and Gabon.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/b/b4/Spilosoma_sp._%285722639526%29.jpg']), ('Kiriakoffalia lemairei', 'Kiriakoffalia lemairei is a moth of the family Erebidae. It was described by Hervé de Toulgoët in 1976. It is found in Cameroon, the Democratic Republic of the Congo and Gabon.', ['https://upload.wikimedia.org/wikipedia/commons/7/74/Red_Pencil_Icon.png', 'https://upload.wikimedia.org/wikipedia/commons/b/b4/Spilosoma_sp._%285722639526%29.jpg'])]