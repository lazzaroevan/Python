[('Gaurena', 'Gaurena is a genus of moths belonging to the subfamily Thyatirinae of the Drepanidae. It was erected by Francis Walker in 1865.', ['https://upload.wikimedia.org/wikipedia/commons/9/95/Thyatira_batis%2C_Lodz%28Poland%2903%28js%29.jpg']), ('Gaurena nigrescens', 'Gaurena nigrescens is a moth in the family Drepanidae. It is found in China (Sichuan).', ['https://upload.wikimedia.org/wikipedia/commons/9/95/Thyatira_batis%2C_Lodz%28Poland%2903%28js%29.jpg']), ('Gaurena pretiosa', 'Gaurena pretiosa is a moth in the family Drepanidae. It is found in China (Yunnan).', ['https://upload.wikimedia.org/wikipedia/commons/9/95/Thyatira_batis%2C_Lodz%28Poland%2903%28js%29.jpg']), ('Gaurena roesleri', 'Gaurena roesleri is a moth in the family Drepanidae. It is found in China (Sichuan).', ['https://upload.wikimedia.org/wikipedia/commons/9/95/Thyatira_batis%2C_Lodz%28Poland%2903%28js%29.jpg']), ('Gaurena sinuata', 'Gaurena sinuata is a moth in the family Drepanidae. It is found in Nepal and China (Sichuan, Shaanxi, Gansu, Yunnan, Tibet).', ['https://upload.wikimedia.org/wikipedia/commons/9/95/Thyatira_batis%2C_Lodz%28Poland%2903%28js%29.jpg'])]